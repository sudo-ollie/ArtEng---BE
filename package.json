    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "compression": "^1.7.4",
    "contentful": "^11.5.24",
    "contentful-management": "^11.52.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "sanitize-html": "^2.17.0",
    "swagger-ui-express": "^5.0.0",
