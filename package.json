    "@types/swagger-ui-express": "^4.1.6",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "openapi-typescript": "^7.0.0",
    "prettier": "^3.2.5",
    "prisma": "^6.8.2",
